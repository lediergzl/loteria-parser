{
  "name": "loteria-parser",
  "version": "1.0.0",
  "description": "Parser avanzado para jugadas de lotería con soporte para múltiples formatos y alta performance",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./parser": {
      "import": "./dist/esm/parser.js",
      "require": "./dist/cjs/parser.js",
      "types": "./dist/types/parser.d.ts"
    },
    "./types": {
      "import": "./dist/esm/types.js",
      "require": "./dist/cjs/types.js",
      "types": "./dist/types/types.d.ts"
    },
    "./utils": {
      "import": "./dist/esm/utils/index.js",
      "require": "./dist/cjs/utils/index.js",
      "types": "./dist/types/utils/index.d.ts"
    },
    "./plugins": {
      "import": "./dist/esm/plugins/index.js",
      "require": "./dist/cjs/plugins/index.js",
      "types": "./dist/types/plugins/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "clean": "rimraf dist coverage",
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc -p tsconfig.json --outDir dist/cjs",
    "build:esm": "tsc -p tsconfig.esm.json --outDir dist/esm",
    "build:types": "tsc -p tsconfig.json --declaration --emitDeclarationOnly --outDir dist/types",
    "build:rollup": "rollup -c",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepublishOnly": "npm run test && npm run build",
    "prepare": "husky install",
    "benchmark": "ts-node benchmarks/parser-benchmark.ts",
    "docs": "typedoc --out docs src/index.ts",
    "example": "ts-node examples/basic-usage.ts",
    "example:web": "node examples/web-usage.js"
  },
  "keywords": [
    "loteria",
    "parser",
    "apuestas",
    "betting",
    "gambling",
    "typescript",
    "javascript",
    "parser-library"
  ],
  "author": "Loteria Parser Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/loteria-parser.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/loteria-parser/issues"
  },
  "homepage": "https://github.com/yourusername/loteria-parser#readme",
  "funding": {
    "type": "patreon",
    "url": "https://patreon.com/yourusername"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "decimal.js": "^10.4.3",
    "lodash.merge": "^4.6.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/jest": "^29.5.5",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@typescript-eslint/typescript-estree": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.0",
    "rollup-plugin-dts": "^6.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "optionalDependencies": {
    "decimal.js": "^10.4.3"
  }
}